{
    "version": 2,
    "name": "UMU API Gateway",
    "port": 8080,
    "cache_ttl": "3600s",
    "timeout": "3s",
    "endpoints": [
        {
            "endpoint": "/user/1",
            "backend": [
                {
                    "host": [
                        "http://your.backend"
                    ],
                    "url_pattern": "/foo",
                    "group": "foo"
                }
            ],
            "extra_config": {
                "github.com/devopsfaith/krakend/proxy": {
                    "static": {
                        "strategy": "errored",
                        "data": {
                            "id_user": 1,
                            "firstName": "John",
                            "secondName": "Smith"
                        }
                    }
                }
            }
        }
    ]
}